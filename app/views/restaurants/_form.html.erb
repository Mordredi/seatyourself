<%= form_for @restaurant do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :image_url %>
  <%= f.text_field :image_url %>
  <%= f.label :seats %>
  <%= f.number_field :seats %>
  <%= f.label :open %>
  <%= f.number_field :open %>
  <%= f.label :close %>
  <%= f.number_field :close %>
  <%= f.label :description %>
  <%= f.text_area :description %>
  <%= f.button %>
<% end %>