<%= form_for @restaurant do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :image_url %>
  <%= f.text_field :image_url %>
  <%= f.label :seats %>
  <%= f.number_field :seats %>
  <%= f.label :open_time %>
  <%= f.time_field :open_time %>
  <%= f.label :close_time %>
  <%= f.time_field :close_time %>
  <%= f.label :description %>
  <%= f.text_area :description %>
  <%= f.button %>
<% end %>